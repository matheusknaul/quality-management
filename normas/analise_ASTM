        function nodeClickingNM(sender, args) {
            var comboBox = $find("ctl00_cphPagina_cboNM_ICS");

            var node = args.get_node()
            comboBox.set_text(node.get_text());
            comboBox.trackChanges();
            comboBox.get_items().getItem(0).set_text(node.get_text());
            comboBox.commitChanges();
            comboBox.hideDropDown();
        }

        function OnClientDropDown_treeNM(sender, eventArgs) {
            var tree = sender.get_items().getItem(0).findControl("treeNM_ICS");
            var selectedNode = tree.get_selectedNode();
            if (selectedNode) {
                selectedNode.scrollIntoView();
            }
        }

        function nodeClickingPR(sender, args) {
            var comboBox = $find("ctl00_cphPagina_cboPR_ICS");

            var node = args.get_node()
            comboBox.set_text(node.get_text());
            comboBox.trackChanges();
            comboBox.get_items().getItem(0).set_text(node.get_text());
            comboBox.commitChanges();
            comboBox.hideDropDown();
        }

        function OnClientDropDown_treePR(sender, eventArgs) {
            var tree = sender.get_items().getItem(0).findControl("treePR_ICS");
            var selectedNode = tree.get_selectedNode();
            if (selectedNode) {
                selectedNode.scrollIntoView();
            }
        }

        function ActivePesquisa() {
            $('.select-active').select2();
        }
    </script>

