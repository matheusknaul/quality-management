    <script type="text/javascript">
        (function (global, undefined) {
            var demo = {};

            function alertCallBackFn(arg) {
            }

            global.alertCallBackFn = alertCallBackFn;
            global.$dialogsDemo = demo;
        })(window); 

        function GoPreview() {
            var txt = document.getElementById('cphPagina_txtAcesso');
            if (txt == null)
                txt = document.getElementById('ctl00_cphPagina_txtAcesso')

            if (txt.value == '') {
                window.location = 'lgn.aspx';
            }
            else if (txt.value == 'ASSOCIADO') {
                window.alert('Serviço de pré-visualização apenas para Associados ABNT. Entre em contato através do email: associados@abnt.org.br e solicite sua filiação.');
            }
            else {
                newWindow = window.open('vf/viewer.aspx?Q=' + txt.value, 'newWin', 'top=0,left=0,width=' + String(screen.width) + ',height=' + String(screen.height) + ', statusbar=no, toolbar=no, addressbar=no, location=no, menubar=no')
            }
        }

        function GoFree() {
            var txt = document.getElementById('cphPagina_txtAcesso');
            if (txt == null)
                txt = document.getElementById('ctl00_cphPagina_txtAcesso')

            newWindow = window.open('vf/viewer.aspx?Q=' + txt.value, 'newWin', 'top=0,left=0,width=' + String(screen.width) + ',height=' + String(screen.height) + ', statusbar=no, toolbar=no, addressbar=no, location=no, menubar=no')            
        }

        function GoHistorico() {
            const element = document.getElementById("acHistorico");
            element.scrollIntoView();
            window.location.hash = 'hideH';
        }
        
    </script>

