    <script type="text/javascript">
        var pnl = document.getElementById('cphPagina_pnlCaptcha');
        if (pnl == null)
            pnl = document.getElementById('ctl00_cphPagina_pnlCaptcha');

        if (pnl != null) {
            var onloadCallback = function () {
                grecaptcha.render('dvCaptcha', {
                    'sitekey': '6Lf1evYcAAAAABCzyhTCZGiGXDcRo-57kVsBvl-O',
                    'callback': function (response) {
                        var obj = document.getElementById('cphPagina_txtCaptcha')
                        if (obj == null)
                            obj = document.getElementById('ctl00_cphPagina_txtCaptcha')

                        if (obj != null)
                            obj.value = response;

                        var rfv = document.getElementById('cphPagina_rfvcaptcha')
                        if (rfv == null)
                            rfv = document.getElementById('ctl00_cphPagina_rfvcaptcha')

                        if (rfv != null)
                            rfv.style.display = 'none';
                    }
                });

                var txt = document.getElementById('cphPagina_txtIdioma');
                if (txt == null)
                    txt = document.getElementById('ctl00_cphPagina_txtIdioma');

                var captcha = document.getElementById('dvCaptcha');
                setCaptchaLang(captcha, txt.value);
            };
        }

        function setCaptchaLang(recaptchaContainer, lang) {
            lang = lang || "en";

            const iframeGoogleCaptcha = recaptchaContainer.querySelector('iframe');
            const currentLang = iframeGoogleCaptcha.getAttribute("src").match(/hl=(.*?)&/).pop();

            if (currentLang !== lang) {
                iframeGoogleCaptcha.setAttribute(
                    "src",
                    iframeGoogleCaptcha.getAttribute("src").replace(
                        /hl=(.*?)&/,
                        'hl=' + lang + '&'
                    )
                );
            }
        }

        function ResetCaptcha() {
            onloadCallback();
        }
    </script>
    

